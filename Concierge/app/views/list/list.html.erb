<script type=text/javascript>


    $.ajax({
        type: "GET",
        url: "http://localhost:3000/peoplelistrequest",
        dataType: "xml",
        success: parseXml
    });

    function parseXml(xml) {
        $(xml).find("item[title='Professor']").each(function() {
            $("#prof_list").append("<li class='arrow'><a href='" + $(this).attr('href') + ">" + $(this).text() + "</a></li>");
        });
    }


</script>